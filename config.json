{
    "$schema": ".github/configSchema.json",
    "maxQueueSize": 1000,
    "volumeModifier": 0.5,
    "colourTheme": "#794c36",
    "levenshteinThreshold": 0.1,
    "announcementSystem": {
        "dontAnnounceOlderThan": 10,
        "enabledInDevelopment": false,
        "enabledInProduction": true
    },
    "timeoutThresholds": {
        "login": 5,
        "connect": 5,
        "generateResource": 5,
        "getSearchResult": 5,
        "inactivity": 60,
        "play": 3,
        "stopQueuePagination": 30,
        "statusUpdate": 3600
    }
}
