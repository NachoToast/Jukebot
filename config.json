{
    "$schema": ".github/configSchema.json",
    "sourceCode":"https://github.com/NachoToast/Jukebot",
    "activityString": "sus remixes",
    "maxQueueSize": 1000,
    "volumeModifier": 0.5,
    "colourTheme": "#794c36",
    "levenshteinThreshold": 0.1,
    "announcementSystem": {
        "dontAnnounceOlderThan": 5,
        "enabledInDevelopment": true,
        "enabledInProduction": true
    },
    "timeoutThresholds": {
        "login": 5,
        "connect": 3,
        "generateResource": 3,
        "getSearchResult": 3,
        "inactivity": 10,
        "play": 3
    }
}
